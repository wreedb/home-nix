{

    ls = "eza --icons --sort=type --color=auto";
    ll = "eza -halF --icons --sort=type --color=auto";
    la = "eza -aaF --icons --sort=type --color=auto";
    l  = "eza -a1F --icons --sort=type --color=auto";

    ln = "ln --verbose --interactive";
    cat = "bat --paging=never --unbuffered --plain";
    grep = "grep --ignore-case --color=auto";
    mkdir = "mkdir --parents --verbose";
    cp = "cp --verbose";
    cpt = "cp --verbose --recursive -t";
    mv = "mv --interactive --verbose";
    mvt = "mv --interactive --verbose -t";
    rg = "rg --ignore-case";
    rgj = "rg --ignore-case --json";
    rm = "rm --verbose";
    rmd = "rm --verbose --recursive --force";

    untar = "tar --extract --verbose -f";
    mktar = "tar --create --verbose -f";
    bld = "mkdir --verbose bld && cd bld";

}